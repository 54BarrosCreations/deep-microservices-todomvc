{
  "preset": "airbnb",
  "requireCamelCaseOrUpperCaseIdentifiers": "ignoreProperties",
  "excludeFiles": [
    "./src/*/Frontend/js/lib/*",
    "./src/*/Tests/Frontend/lib/*",
    "./src/*/Tests/Frontend/mock/lib/*",
    "./src/*/Tests/Frontend/vendor/*"
  ],
  "requirePaddingNewLinesAfterBlocks": {
    "allExcept": [
      "inCallExpressions"
    ]
  },
  "mocha": true,
  "jasmine": true,
  "globals": {
    "after": false,
    "afterEach": false,
    "before": false,
    "beforeEach": false,
    "by": false,
    "browser": false,
    "describe": false,
    "driver": false,
    "expect": false,
    "element": false,
    "inject": false,
    "it": false,
    "list": false,
    "protractor": false
  }
}